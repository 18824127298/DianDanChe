<%@ Page Language="C#" AutoEventWireup="true" CodeFile="dns_trans_code_search.aspx.cs" Inherits="genui_LNWX_dns_trans_code_search" %>

<html>
<head runat="server">
    <title>搜索服务交易码</title>
</head>
<body>
    <form id="form1" runat="server">
    <div>
        <div class="titleTable">
            <table border="0" cellpadding="3" cellspacing="0" width="100%">
                <tr>
                    <td>
                        <img src="../../images/menu_icon/plug.gif" />
                        搜索服务交易码</td>
                </tr>
            </table>
            <hr />
        </div>

        <div class="contentTable">
        <br />
        <table border="0" width="450" cellpadding="4" cellspacing="1" align="center">
        <tbody>
            <tr>
                <td nowrap>
                    交易码：</td>
                <td>
                    &nbsp;<asp:TextBox ID="edtTransCode" runat="server" Width="200px"></asp:TextBox></td>
            </tr>
            <tr>
                <td nowrap>
                    名称：</td>
                <td>
                    &nbsp;<asp:TextBox ID="edtTransCodeName" runat="server" Width="200px"></asp:TextBox></td>
            </tr>
            <tr>
                <td nowrap>
                    寻址服务：</td>
                <td>
                    &nbsp;<asp:DropDownList ID="ddlbServiceId" runat="server" Width="200px"></asp:DropDownList></td>
            </tr>
        </tbody>
        <tfoot>
            <tr align="center">
                <td colspan="2" nowrap>
                    &nbsp;<asp:Button ID="btnOK" runat="server" Text="确定" OnClick="btnOK_Click" />
                    &nbsp; &nbsp; &nbsp;
                    <input id="btnCancel" type="button" value="取消" language="javascript" onclick="history.back()" class="normalButton" /></td>
            </tr>
        </tfoot>
        </table>
        </div>
    </div>
    </form>
</body>
</html>
