using System;
using System.Collections;
using System.Collections.Generic;
using System.Text;

namespace Sigbit.Common.WordProcess
{
    /// <summary>
    /// 模板标识数据机类
    /// </summary>
    public class ExcelExportIndicatorItem
    {
        #region 私有属性
        
        /// <summary>
        /// 行
        /// </summary>
        private int _row = 0;

        /// <summary>
        /// 列
        /// </summary>
        private int _col = 0;
        
        /// <summary>
        /// 标识
        /// </summary>
        private string _indicator = "";
        
        /// <summary>
        /// 显示文本
        /// </summary>
        private string _text = "";


        #endregion 私有属性

        #region 私有方法

        #endregion 私有方法

        #region 公有属性
        /// <summary>
        /// 数据行
        /// </summary>
        public int Row
        {
            get { return _row; }
            set { _row = value; }
        }

        /// <summary>
        /// 数据列
        /// </summary>
        public int Col
        {
            get { return _col; }
            set { _col = value; }
        }

        /// <summary>
        /// 标识名称
        /// </summary>
        public string Indicator
        {
            get { return _indicator; }
            set { _indicator = value; }
        }

        /// <summary>
        /// 替换文本
        /// </summary>
        public string Text
        {
            get { return _text; }
            set { _text = value; }
        }
        #endregion 公有属性

        #region 公有方法
        #endregion 公有方法
    }
}
